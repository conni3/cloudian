{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "hugo",
  "buildCommand": "hugo --environment production --gc --minify",
  "outputDirectory": "public",
  "build": {
    "env": {
      "HUGO_VERSION":      "0.110.0",
      "HUGO_ENVIRONMENT":  "production",
      "HUGO_BASEURL":      "https://cloudian-sand.vercel.app/"
    }
  },
  "cleanUrls": true
}
